000019r 3               ; Print a string
000019r 3               ; Pass address of string in X (low) and Y (high).
000019r 3               ; String must be terminated in a null.
000019r 3               ; Cannot be longer than 256 characters.
000019r 3               ; Registers changed: A, Y
000019r 3               ;
000019r 3               PrintString:
000019r 3  86 A3                STX TEMP1
00001Br 3  84 A4                STY TEMP1+1
00001Dr 3  A0 00                LDY #0
00001Fr 3  B1 A3        @loop:  LDA (TEMP1),Y
000021r 3  F0 06                BEQ done
000023r 3  20 rr rr             JSR MONCOUT
000026r 3  C8                   INY
000027r 3  D0 F6                BNE @loop       ; if doesn't branch, string is too long
000029r 3  60           done:   RTS
00002Ar 3               
00002Ar 3               ; String input routine.
00002Ar 3               ; Enter characters from the keyboard terminated in <Return> or <ESC>.
00002Ar 3               ; Characters are echoed.
00002Ar 3               ; Can be up to 127 characters.
00002Ar 3               ; Returns:
00002Ar 3               ;   Length stored at IN (doesn't include zero byte).
00002Ar 3               ;   Characters stored starting at IN+1 ($0201-$027F, same as Woz Monitor)
00002Ar 3               ;   String is terminated in a 0 byte.
00002Ar 3               ;   Carry set if user hit <Esc>, clear if used <Enter> or max string length reached.
00002Ar 3               ; Registers changed: A, X
00002Ar 3               GetLine:
00002Ar 3  A2 00                LDX  #0                 ; Initialize index into buffer
00002Cr 3               loop:
00002Cr 3  20 rr rr             JSR  MONRDKEY		; Get character from keyboard
00002Fr 3  29 7F                AND  #$7F               ; Convert to ASCII
000031r 3  C9 0D                CMP  #CR                ; <Enter> key pressed?
000033r 3  F0 11                BEQ  EnterPressed       ; If so, handle it
000035r 3  C9 1B                CMP  #ESC               ; <Esc> key pressed?
000037r 3  F0 0E                BEQ  EscapePressed      ; If so, handle it
000039r 3  20 rr rr             JSR  MONCOUT            ; Echo the key pressed
00003Cr 3  9D 01 02             STA  IN+1,X             ; Store character in buffer (skip first length byte)
00003Fr 3  E8                   INX                     ; Advance index into buffer
000040r 3  E0 7E                CPX  #$7E               ; Buffer full?
000042r 3  F0 02                BEQ  EnterPressed       ; If so, return as if <Enter> was pressed
000044r 3  D0 E6                BNE  loop               ; Always taken
000046r 3               EnterPressed:
000046r 3  18                   CLC                     ; Clear carry to indicate <Enter> pressed and fall through
000047r 3               EscapePressed:
000047r 3  A9 00                LDA  #0
000049r 3  9D 01 02             STA  IN+1,X             ; Store 0 at end of buffer
00004Cr 3  8E 00 02             STX  IN                 ; Store length of string
00004Fr 3  60                   RTS                     ; Return
